{
  "name": "craft-vue-tailwind",
  "title": "Craft Vue Tailwind",
  "version": "3.0.1",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build && fractal build && yarn build:components",
    "build:components": "fractal pathmap",
    "build:fractal": "fractal build",
    "generate:component": "hygen fractal-component new",
    "lint": "vue-cli-service lint",
    "lint:autofix": "vue-cli-service lint --fix",
    "lint:config-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "project:release": "./scripts/deploy/release",
    "project:configure": "./scripts/project/configure",
    "project:after-install": "./scripts/project/after-install",
    "project:apply-env": "./scripts/project/apply-env",
    "project:setup-craft": "./scripts/project/setup-craft",
    "serve": "concurrently \"vue-cli-service serve\" \"fractal start\"",
    "serve:assets": "vue-cli-service serve",
    "serve:craft": "nanobox run php-server",
    "serve:fractal": "fractal start",
    "test:coverage": "jest --coverage",
    "test:unit": "vue-cli-service test:unit",
    "test:watch": "vue-cli-service test:unit --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:autofix",
      "pre-push": "yarn lint && yarn test:unit"
    }
  },
  "dependencies": {
    "@babel/core": "^7.0.0-0",
    "@frctl/fractal": "^1.1.7",
    "@frctl/mandelbrot": "^1.2.0",
    "@frctl/twig": "^1.0.0-beta.5",
    "@fullhuman/postcss-purgecss": "^1.1.0",
    "autoprefixer": "^9.4.7",
    "cli-plugin-eslint": "^0.0.1",
    "colors": "^1.3.3",
    "concurrently": "^4.1.0",
    "dotenv": "^6.2.0",
    "postcss-import": "^12.0.0",
    "postcss-preset-env": "^6.5.0",
    "tailwindcss": "^0.7.3",
    "vue": "^2.5.16",
    "webpack": "^4.4.0"
  },
  "devDependencies": {
    "@vue/babel-preset-app": "^3.4.0",
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-plugin-unit-jest": "^3.4.0",
    "@vue/cli-service": "^3.0.0-rc.10",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "backstopjs": "^3.8.8",
    "commander": "^2.19.0",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.0.0",
    "glob-all": "^3.1.0",
    "husky": "^1.3.1",
    "hygen": "^2.1.2",
    "inquirer": "^6.2.2",
    "jsonfile": "^5.0.0",
    "mini-css-extract-plugin": "^0.4.1",
    "postcss-nested": "^4.1.2",
    "prettier": "^1.16.4",
    "prettier-stylelint": "^0.4.2",
    "purgecss-whitelister": "^2.1.0",
    "randomatic": "^3.1.1",
    "sane": "^3.0.0",
    "svgo": "^1.1.0",
    "svgo-loader": "^2.1.0",
    "twig": "^1.13.2",
    "vue-cli-plugin-svg-sprite": "^0.3.1",
    "vue-template-compiler": "^2.5.16",
    "webpack-manifest-plugin": "^2.0.3"
  }
}
